    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vitest": "^0.7.6"
  }
